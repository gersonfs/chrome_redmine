{
    "manifest_version": 2,
    "name": "OneHost Projects",
    "version": "0.2.12",
    "description": "Esta extensão permite integrar o Google Chrome™ com o Redmine",
    "permissions": [
        "http://*/*",
        "https://*/*",
        "tabs"
    ],
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "img/icon48.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "options_page": "options.html",
    "author": "Gerson Felipe Schwinn <gersonfs@gmail.com>",
    "homepage_url": "https://github.com/gersonfs"
}